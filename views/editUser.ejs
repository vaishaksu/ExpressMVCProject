<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="css/login.css">
    </head>
    <body>
        <div class="login-box">
            <h2>Edit User</h2>
            <form action="PostEditUser/<%= user.username %>" method="post">
              <div class="user-box">
                <input type="text" name="first_name" required="" value="<%= user.first_name%>">
                <label>First Name</label>
              </div>

              <div class="user-box">
                <input type="text" name="last_name" required="" value="<%= user.last_name%>">
                <label>Last Name</label>
              </div>

              <div class="user-box">
                <input type="email" name="email" required="" value="<%= user.email%>">
                <label>Email</label>
              </div>

              <div class="user-box">
                <input type="password" name="password" required="" value="<%= user.password%>">
                <label>Password</label>
              </div>

              <!-- <a href="#">
                <span></span>
                <span></span>
                <span></span>
                <span></span>
                SignUp
              </a> -->
              <input type="submit" value="Submit"/>
            </form>
          </div>
    </body>
</html>